<table class="genre-search-table">
    <tr><th>ジャンル検索</th></tr>
    <% genres.each do |genre| %>
    <tr>
        <td class="pl-3 pt-2 pb-2">
            <%= link_to items_path(genre_id: genre.id), method: :get do %>
                <%= genre.name %>
            <% end %>
        </td>
    </tr>
    <% end %>
</table>